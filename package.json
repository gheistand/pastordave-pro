{
  "name": "pastordave-pro",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler pages dev . --compatibility-date=2024-01-01",
    "deploy": "wrangler pages deploy .",
    "db:migrate": "wrangler d1 execute pastordave-pro --file=./migrations/0001_init.sql",
    "db:migrate:remote": "wrangler d1 execute pastordave-pro --file=./migrations/0001_init.sql --remote"
  },
  "devDependencies": {
    "wrangler": "latest"
  },
  "dependencies": {
    "@clerk/backend": "^1.0.0",
    "stripe": "^14.0.0"
  }
}
